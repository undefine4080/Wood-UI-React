{
    "name": "wood-ui-react",
    "version": "0.0.60",
    "description": "a ui library build by react and typescript",
    "author": "ztf",
    "license": "MIT",
    "types": "./build/index.d.ts",
    "keywords": [
        "react",
        "typescript",
        "ui library"
    ],
    "homepage": "https://zhengtengfei.gitee.io/wood-ui-react/",
    "repository": "https://github.com/undefine4080/Wood-UI-React",
    "files": [
        "build"
    ],
    "module": "./build/wood-ui.es.js",
    "main": "./build/wood-ui.umd.js",
    "exports": {
        ".": {
            "import": "./build/wood-ui.es.js",
            "require": "./build/wood-ui.umd.js"
        },
        "./style": {
            "import": "./build/style.css",
            "require": "./build/style.css"
        }
    },
    "scripts": {
        "dev": "vite",
        "clear": "vite --force",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@testing-library/react": "^13.4.0",
        "jsdom": "^20.0.1",
        "monaco-editor": "^0.34.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.6.1",
        "vite-plugin-dts": "^2.3.0"
    },
    "devDependencies": {
        "@types/node": "^18.7.18",
        "@types/react": "^18.0.20",
        "@types/react-dom": "^18.0.6",
        "@vitejs/plugin-react": "^2.1.0",
        "less": "^4.1.3",
        "typescript": "^4.8.3",
        "vite": "^3.2.5",
        "vite-plugin-progress": "^0.0.7",
        "vitest": "^0.26.3"
    }
}
